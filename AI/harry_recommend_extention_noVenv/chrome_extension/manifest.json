{
    "manifest_version": 3,
    "name": "harry",
    "version": "1.0",
    "description": "사용자 관심사 맞춤형 페이지 추천 서비스",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icons/icon.png"
    }
  }
  